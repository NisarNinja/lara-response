{
    "name": "nisarr/lara-response",
    "description": "Laravel response.",
    "keywords": [
        "nisarr",
        "lara-response"
    ],
    "homepage": "https://github.com/nisarr/lara-response",
    "license": "MIT",
    "type": "library",
    "authors": [{
        "name": "Nisar Ahmed",
        "email": "nisar.ahmed.dev@gmail.com",
        "role": "Full stack web developer"
    }],
    "require": {
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Nisarr\\LaraResponse\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nisarr\\LaraResponse\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nisarr\\LaraResponse\\LaraResponseServiceProvider"
            ]
        }
    }
}